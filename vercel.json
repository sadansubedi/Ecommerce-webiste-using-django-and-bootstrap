
{
    "builds": [{
        "src": "Ecommerce-webiste-using-django-and-bootstrap/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Ecommerce-webiste-using-django-and-bootstrap/wsgi.py"
        },
         {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
    ]
}

